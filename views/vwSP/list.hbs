<div class="card">
  <h5 class="card-header">Danh sách sản phẩm</h5>
  <div class="card-body">
   <table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Tên sản phẩm</th>
      <th scope="col">Đơn vị định lượng</th>
      <th scope="col">Giá tiền</th>
    </tr>
  </thead>
  <tbody>
   {{#each spList}}
        <tr>
            <th scope="row">{{this.IDSanPham}}</th>
            <th scope="row">{{this.TenSanPham}}</th>
            <th scope="row">{{this.GiaTien}}</th>
            <th scope="row">{{this.DonViDinhLuong}}</th>
        </tr>
   {{/each}}
  </tbody>
</table>
  </div>
</div>