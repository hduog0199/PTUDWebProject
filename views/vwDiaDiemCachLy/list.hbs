<div class="card">
  <h5 class="card-header">Danh sách địa điểm cách ly</h5>
  <div class="card-body">
   <table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Sức chứa</th>
      <th scope="col">Số lượng tiếp nhận</th>
    </tr>
  </thead>
  <tbody>
   {{#each listDiaDiemCachLy}}
        <tr>
            <th scope="row">{{this.IDKhuCachLy}}</th>
            <th scope="row">{{this.Name}}</th>
            <th scope="row">{{this.SucChua}}</th>
            <th scope="row">{{this.SoLuongTiepNhan}}</th>
        </tr>
   {{/each}}
  </tbody>
</table>
  </div>
</div>